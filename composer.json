{
    "name": "cristiangomeze/template",
    "description": "Convert word templates to pdf",
    "license": "MIT",
    "authors": [
        {
            "name": "Cristian Gomez",
            "email": "cristiangomeze@hotmail.com"
        }
    ],
    "homepage": "https://github.com/cristiangomeze/template",
    "keywords": ["Laravel", "Template"],
    "require": {
        "illuminate/support": "^8.0|^9.0",
        "symfony/process": "^6.0",
        "kwn/number-to-words": "^2.0",
        "phpoffice/phpword": "^0.18"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0",
        "sempro/phpunit-pretty-print": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Cristiangomeze\\Template\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cristiangomeze\\Template\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cristiangomeze\\Template\\TemplateServiceProvider"
            ],
            "aliases": {
                "Template": "Cristiangomeze\\Template\\Facades\\Template"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
